#!/bin/sh

### Changelog #################################################################

# 31/08/2005 - [lucasvr] First version

### Operation #################################################################
if [ $# != 1 ]
then
	echo "Syntax: $0 <filename>"
	exit 1
fi

echo "file_size=`ls -l $1 | awk {'print $5'}`"
echo "file_md5=`md5sum "$1" | awk {'print $1'}`"
